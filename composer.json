{
    "name": "cubeta/cubeta-starter",
    "description": "Project starter kit to generate everything and make things easier!",
    "license": "MIT",
    "authors": [
        {
            "name": "cubeta",
            "email": "development@cubeta.io"
        }
    ],
    "require": {
        "intervention/image": "^2.7",
        "spatie/laravel-package-tools": "^1.11",
        "illuminate/support": "*",
        "php-open-source-saver/jwt-auth": "^2.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.5",
        "laravel/pint": "^1.7"
    },
    "autoload": {
        "files": [
            "src/Helpers/NamingHelpers.php",
            "src/Helpers/FilesHelper.php"
        ],
        "psr-4": {
            "Cubeta\\CubetaStarter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cubeta\\CubetaStarter\\tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cubeta\\CubetaStarter\\CubetaStarterServiceProvider",
                "Cubeta\\CubetaStarter\\RepositoryServiceProvider"
            ]
        }
    }
}
